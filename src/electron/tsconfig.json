{
  "compilerOptions": {
    //require strict types (null-save)
    "strict": true, 
    // tell typescript to generate ESM syntax
    "target": "ESNext", 
    // tell typescript to require ESM syntax as Input
    "module": "NodeNext", 
    // define where to put generated JS 
    "outDir": "../../dist-electron", 
    //ignore error from dependencies 
    "skipLibCheck": true
  }
}